@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@Resources.Base.Common.Privacy</h1>

<p>@Resources.Base.Common.PrivacyDescription</p>

<h4>@Resources.Base.Common.PrivacyUserInfo</h4>
<table class="table table-striped">
    <thead>
    <tr>
        <th>@Resources.Base.Common.PrivacyType</th>
        <th>@Resources.Base.Common.PrivacyValue</th>
        <th>@Resources.Base.Common.PrivacyValueType</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var claim in User.Claims)
    {
        <tr>
            <td>
                @claim.Type
            </td>
            <td>
                @claim.Value
            </td>
            <td>
                @claim.ValueType
            </td>
        </tr>
    }
    </tbody>
</table>

