@model WebApp.ViewModels.Test.TestViewModel

@{
    ViewData["Title"] = "Test";
}

<h1>Test</h1>


<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Persons.FirstOrDefault()!.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Persons.FirstOrDefault()!.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Persons.FirstOrDefault()!.PersonsIdCode)
        </th>
        
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model.Persons)
    {
        <tr>
            <td>
                @item.FirstName
            </td>
            <td>
                @item.LastName
            </td>
            <td>
                @item.PersonsIdCode
            </td>
            
        </tr>
    }
    </tbody>
</table>