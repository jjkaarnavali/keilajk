@model WebApp.ViewModels.Test.TestViewModel

@{
    ViewData["Title"] = "Test";
}

<h1>Test</h1>


<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PaymentTypes.FirstOrDefault()!.PaymentTypeName)
        </th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model.PaymentTypes)
    {
        <tr>
            <td>
                @item.PaymentTypeName
            </td>
        </tr>
    }
    </tbody>
</table>